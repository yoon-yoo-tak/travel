<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tak.login.ILoginDAO">
<select id="loginProc" parameterType="com.tak.login.Login" resultType="int">
select count(*)
from login
where id=#{id}
and pw=#{pw}
</select>
</mapper>